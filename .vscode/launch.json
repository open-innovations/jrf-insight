  "version": "0.2.0",
  "configurations": [
    {
      "request": "launch",
      "name": "Profile lume build",
      "type": "node",
      "program": "${workspaceFolder}/profile.ts",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "/home/giles/.deno/bin/deno",
      // "profileStartup": true,
      "env": {
        "DENO_V8_FLAGS": "--max-old-space-size=8192",
      },
      "runtimeArgs": [
        "run",
        "--unstable",
        // "--import-map",
        // "./import_map.json",
        "--config",
        "./deno.json",
        "--inspect-brk",
        "--allow-all"
      ],
      "attachSimplePort": 9229
